my_string = "Hello World"

print(my_string)
print(type(my_string))

my_string = "Hello \nWorld"

print(my_string)

my_string = "Hello \tWorld"

print(type(my_string))

my_string = "Hello ''World''"

print(type(my_string))

my_string = "Hello \"World\""

test_string = "Hello"
print(test_string[1])
test_string = test_string.replace("o", "*")
print(test_string)

my_sentence = "This is a sentance"
print(len(my_sentence))

string_1 = "Hello "
string_2 = "World!"
string_3 = string_1 + string_2
print(string_3)
print(string_3[3] + string_3[6])

sentence = "Wow, What a large horse!"
sentence = sentence.replace("large", "small")
print(sentence)

my_filepath = "C:/Images/cow.jpg"
my_filestructure = my_filepath.split("/")
print(my_filestructure[2])