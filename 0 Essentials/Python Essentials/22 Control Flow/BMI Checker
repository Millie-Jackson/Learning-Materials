
def calculate_bmi(weight, height):
    return round(weight / height ** 2)

def catagorize_bmi(bmi):
    if bmi < 18.5:
        print(f"Your BMI is {bmi}. Your in the underweight range")
    elif bmi > 18.5 and bmi < 24.9:
        print(f"Your BMI is {bmi}. Your in the healthy weight range")
    elif bmi > 25 and bmi < 29.9:
        print(f"Your BMI is {bmi}. Your in the overweight range")
    else:
        print(f"Your BMI is {bmi}. Your in the obese range")

catagorize_bmi(calculate_bmi(85, 1.83))
catagorize_bmi(calculate_bmi(61, 1.55))
catagorize_bmi(calculate_bmi(135, 2.09))
